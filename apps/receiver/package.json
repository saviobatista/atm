    "amqplib": "^0.10.3",
    "axios": "^1.2.5",
    "logger": "*",
    "morgan": "^1.10.0",
    "mssql": "^7.2.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.0",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "*",
    "jest": "^26.6.3",
    "jest-presets": "*",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.3"
