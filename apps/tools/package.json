{
  "name": "tools",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node -r esbuild-register ./src/index.ts",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "mssql": "^9.0.1",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "*",
    "jest": "^26.6.3",
    "jest-presets": "*",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  }
}
